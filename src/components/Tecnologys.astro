            <p class="mb-1 text-xl font-medium text-gray-900 dark:text-white"> Tailwind Css</p>
    </div>
</div>
<!-- Segunda columna -->
<div class="flex flex-col md:flex-row items-center justify-center gap-6 my-5" >

    <div class="flex flex-col  items-center rounded-xl  hover:shadow-xl hover:transform hover:scale-110 duration-200 pb-10 w-80  md:w-40 max-w-sm  bg-white border border-gray-200  shadow dark:bg-gray-900 dark:border-gray-800 ">
        <Image class="w-24 h-24 my-5" src={Sass} alt="logo Sass"/>
        <h5 class="mb-1 text-xl font-medium text-gray-900 dark:text-white"> Sass</h5>
    </div>
    <div class="flex flex-col  items-center rounded-xl  hover:shadow-xl hover:transform hover:scale-110 duration-200 pb-10 w-80  md:w-40 max-w-sm  bg-white border border-gray-200  shadow dark:bg-gray-900 dark:border-gray-800 ">
            <Image class="w-20 h-24 my-5" src={Gulp} alt="logo Gulp"/>
